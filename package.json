{
    "scripts": {
        "install-backend": "cd udagram-api && npm install",
        "install-frontend": "cd udagram-frontend && npm install --force",
        "build-backend": "cd udagram-api && npm run build",
        "build-frontend": "cd udagram-frontend && npm run build",
        "test-frontend": "cd udagram-frontend && npm run test",
        "start-server": "cd udagram-api && npm run dev",
        "test-end-to-end": "cd udagram-frontend && npm run e2e",
        "deploy-backend": "cd udagram-api && npm run back_deploy",
        "deploy-frontend": "cd udagram-frontend && npm run front_deploy"
    },
    "devDependencies": {
        "dotenv": "^16.0.0"
    }
}
